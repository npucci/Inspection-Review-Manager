<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nicco.inspectionReviewManager">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <application
        android:name="com.example.nicco.inspectionReviewManager.customDatatypes.Model"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:installLocation="preferExternal"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.nicco.inspectionReviewManager.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.MainActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.MainActivity" />
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.ProjectActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity" />
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.DateActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity" />
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.ConcreteActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity" />
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.FramingActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity" />
        </activity>
        <activity
            android:name="com.example.nicco.inspectionReviewManager.activities.ConclusionActivity"
            android:parentActivityName="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity"
            android:launchMode="singleTask" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.nicco.inspectionReviewManager.activities.InspectionReviewActivity" />
        </activity>
    </application>

</manifest>